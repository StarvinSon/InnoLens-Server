{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "run",
        "start:debug"
      ],
      "port": 9229,
      "restart": true,
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Server",
      "processId": "${command:PickProcess}"
    }
  ]
}
